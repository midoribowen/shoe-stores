<!-- Update Store Name Form: btn-warning -->
<h5><u>Update A Store</u></h5>
    <form action="/store/$store.getId()" method="post" class="form-horizontal">
      <input id="storeId" name="storeId" type="hidden" value="$store.getId()">
      <div class="form-group">
        <label for="new-name">New Name</label>
        <input id="new-name" name="new-name" type="text" class="form-control">
      </div>
      <br>
      <button type="submit" class="btn btn-warning update-store">Submit</button>
    </form>
  </div>
